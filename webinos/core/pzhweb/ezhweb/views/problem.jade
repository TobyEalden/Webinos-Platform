extends layout

block content
  - var optionTheme = (typeof ui === "undefined") ? "d" : ui.optionTheme;
  div(data-role='content')
    p There was a problem...
    p !{error}
    a(href="#", data-rel="back", data-role="button", data-icon="arrow-r", data-iconpos="right", data-inline="true", data-theme="#{optionTheme}") continue